{
  "headers": [
    {
      "source": "/(.*)\\.(css|js|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "hosseintaheri.org"
        }
      ],
      "destination": "https://www.hosseintaheri.org/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "scheme",
          "value": "http"
        }
      ],
      "destination": "https://www.hosseintaheri.org/:path*",
      "permanent": true
    }
  ]
}